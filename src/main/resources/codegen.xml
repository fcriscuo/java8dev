<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration xmlns="http://www.jooq.org/xsd/jooq-codegen-2.3.0.xsd">
    <jdbc>
        <driver>org.h2.Driver</driver>
        <url>jdbc:h2:tcp://localhost//data/cohort_study/h2db/cohortstudy</url>
        <user>cdrn</user>
        <password>cdrn</password>
    </jdbc>
    <generator>
        <name>org.jooq.util.DefaultGenerator</name>
        <database>
            <name>org.jooq.util.h2.H2Database</name>
            <includes>.*</includes>
            <excludes></excludes>
            <inputSchema>COHORT_STUDY</inputSchema>
        </database>
        <generate></generate>
        <target>
            <packageName>org.nygenome.cdrn.nyc.sitestudy.h2.generated</packageName>
            <directory>./src</directory>
        </target>
    </generator>
</configuration>
